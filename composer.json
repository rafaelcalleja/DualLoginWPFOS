{
    "name": "rc/duallogin-wp-fos-bundle",
    "type": "symfony-bundle",
    "description": "Simultaneous login fosuserbundle and hypebeastwordpressbundle",
    "keywords": ["authentication", "symfony2", "provider", "wordpress", "FOS", "bundle"],
    "homepage": "https://github.com/rafaelcalleja/DualLoginWPFOS.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Rafael Calleja",
            "email": "rafa.calleja@d-noise.net"
        }
    ],
    "require": {
        "php": ">=5.3.2",
	"friendsofsymfony/user-bundle": "dev-master",
	"hypebeast/wordpress-bundle": "*"
	
    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.1,<2.2-dev"
    },
    "autoload": {
        "psr-0": { "RC\\DualLoginWPFOSBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "RC/DualLoginWPFOSBundle"
}
